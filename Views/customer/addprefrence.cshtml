
@{
    ViewData["Title"] = "addprefrence";
    Layout = "~/Views/Layouts/_adminLayout.cshtml";
}


<div class="">


    <div class="container-fluid">


        <br />
        <br />
        <div class="row">
            <div class="col-12">
                <div class="card m-b-30">
                    <div class="card-body">

                        <h4 class="mt-0 header-title" style=" text-align: center; color: black; font-size: 30px;">Add prefrence </h4>
                        <br />
                        <br />

                        <form action="#" enctype="multipart/form-data" method="post" id="studenteForm">
                            <div>


                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group p-0">
                                            <label>Flight Date</label>
                                            <input name="date" class="form-control" type="datetime-local" value="2011-08-19T13:45:00" id="example-datetime-local-input">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group p-0">
                                            <label>Available Weight</label>
                                            <input id="tch3" type="text" name="tch3_22" data-bts-button-down-class="btn btn-secondary btn-outline" data-bts-button-up-class="btn btn-secondary btn-outline" class="form-control" style="display: block;">
                                        </div>


                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>To airport</label>
                                            <select name="toairport" class="form-control mb-3 "  style="width: 100%; height:36px;">
                                                <option value="Egypt">Egypt</option>
                                                <option value="Airport1">Airport1</option>
                                                <option value="Airport2">Airport2</option>

                                            </select>

                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>Country</label>
                                            <select name="tocountry" class="form-control mb-3 " style="width: 100%; height:36px;">
                                                <option value="Egypt">Egypt</option>
                                                <option value="pakistan">pakistan</option>
                                                <option value="USA">USA</option>

                                            </select>

                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>City</label>
                                            <select name="tocity" class="form-control mb-3" style="width: 100%; height:36px;">
                                                <option value="Lahore">Lahore</option>
                                                <option value="Islamabad">Islamabad</option>
                                                <option value="Bahawalpur">Bahawalpur</option>

                                            </select>

                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>From airport</label>
                                            <select name="fromairport" class="form-control mb-3" style="width: 100%; height:36px;">
                                                <option value="Egypt">Egypt</option>
                                                <option value="Airport1">Airport1</option>
                                                <option value="Airport2">Airport2</option>

                                            </select>

                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>Country</label>
                                            <select name="fromcountry" class="form-control mb-3" style="width: 100%; height:36px;">
                                                <option value="Egypt">Egypt</option>
                                                <option value="pakistan">pakistan</option>
                                                <option value="USA">USA</option>

                                            </select>

                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group p-0">
                                            <label>City</label>
                                            <select name="fromcity" class="form-control mb-3" style="width: 100%; height:36px;">
                                                <option value="Lahore">Lahore</option>
                                                <option value="Islamabad">Islamabad</option>
                                                <option value="Bahawalpur">Bahawalpur</option>

                                            </select>

                                        </div>
                                    </div>

                                </div>
                            </div>



                            <br />

                            <div class="form-group p-0" style="text-align: center;">
                                <a href="#" class="btn btn-success" ><i class="" id="AddToCart"></i>Add To Cart</a>
                                @*<a onclick="SendData()" class="btn btn-primary waves-effect waves-light">Add prefrence</a>*@
                            </div>

                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script type="text/javascript">

    function SendData() {
        
      
        
        //console.log(data);
        //alert(data);
        var formData = {};
        var formData= $("#studenteForm").serialize();
        // formData.date = $("#example-datetime-local-input").val();
        //formData.tch3_22 = $("#tch3").val();
        alert(formData);
            @*formData.ProductCode = '@Model.ProductCode';
            formData.ImagePath = '@Model.ItemImageByPath';
            formData.retail = '@Model.retail';
            formData.total = $("#result").text();
            formData.Quantity = $("#Quantity").val();*@
        $.ajax(
            {
                type: "POST",
                url: '/customer/submitpreference',
                        @*url: '@Url.Action("AddToCart", "Cart", new{@area= "Cart" })',*@
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8', // when we use .serialize() this generates the data in query string format. this needs the default contentType (default content type is: contentType: 'application/x-www-form-urlencoded; charset=UTF-8') so it is optional, you can remove it
                data: formData,
                success: function (result) {
                    alert('Successfully received Data ');
                    console.log(result);
                },
                error: function () {
                    alert('Failed to receive the Data');
                    console.log('Failed ');
                }
            });

    }
</script>


