
@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Layouts/_adminLayout.cshtml";
}


<div class="page-wrapper">


    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 style=" font-size: 25px; font-weight: bold;" class="card-title">OFFER INFO'S</h4>
                        
                        <div class="table-responsive m-t-40">
                            <table class="demo-foo-addrow2 table table-bordered table m-b-0 toggle-arrow-tiny example23" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th data-toggle="true">First Name</th>
                                        <th>Last Name</th>
                                        <th data-hide="phone"> Address</th>

                                        <th data-hide="all">Zip Code</th>
                                        <th data-hide="all">Phone</th>
                                        <th data-hide="all">E-mail</th>
                                    </tr>
                                </thead>
                               
                                <tbody>

                                    @foreach (var i in ViewBag.users)
                                    {
                                        <tr>

                                            <td>

                                                <a>@i.First_Name</a>

                                            <td>
                                                <a href="javascript:void(0);">@i.Last_Name</a>
                                            </td>

                                            <td>@i.address</td>
                                            <td>@i.zip_code</td>
                                            <td>@i.phone</td>
                                            <td>@i.email</td>





                                        </tr>

                                    }
                                </tbody>

                            </table>
                        </div>
                        <script type="text/javascript">
                            function checkDelete() {
                                if (confirm('Are you sure you want to delete ?')) {

                                    return true
                                }
                                else {
                                    return false
                                }
                            }
                        </script>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>


